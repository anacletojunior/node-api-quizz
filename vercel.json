{
  "version": 2,
  "builds": [
    {
      "src": "api/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/quizz",
      "dest": "/api/server.js"
    },
    {
      "src": "/update",
      "dest": "/api/server.js",
      "methods": ["PUT"]
    },
    {
      "src": "/(.*)",
      "dest": "/api/server.js"
    }
  ]
}
